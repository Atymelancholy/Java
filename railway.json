{
  "build": {
    "builder": "docker",
    "config": {
      "dockerfile": "Dockerfile"
    }
  },
  "deploy": {
    "startCommand": "java -XX:+UseContainerSupport -XX:MaxRAMPercentage=75 -jar app.jar",
    "environment": {
      "RAILWAY": "true",
      "SPRING_PROFILES_ACTIVE": "prod"
    }
  }
}